<variant
    name="release"
    package="com.beeper.android_seshat"
    minSdkVersion="21"
    targetSdkVersion="31"
    debuggable="true"
    mergedManifest="lint/intermediates/merged_manifest/release/AndroidManifest.xml"
    manifestMergeReport="lint/outputs/logs/manifest-merger-release-report.txt"
    proguardFiles="lint/intermediates/default_proguard_files/global/proguard-android-optimize.txt-7.0.0:proguard-rules.pro"
    consumerProguardFiles="consumer-rules.pro"
    partialResultsDir="lint/intermediates/lint_partial_results/release/out">
  <buildFeatures
      namespacing="REQUIRED"/>
  <sourceProviders>
    <sourceProvider
        manifest="src/main/AndroidManifest.xml"
        javaDirectories="src/main/java:src/main/kotlin"
        resDirectories="src/main/res"
        assetsDirectories="src/main/assets"/>
    <sourceProvider
        manifest="src/release/AndroidManifest.xml"
        javaDirectories="src/release/java:src/release/kotlin"
        resDirectories="src/release/res"
        assetsDirectories="src/release/assets"/>
  </sourceProviders>
  <testSourceProviders>
    <sourceProvider
        manifest="src/test/AndroidManifest.xml"
        javaDirectories="src/test/java:src/test/kotlin"
        resDirectories="src/test/res"
        assetsDirectories="src/test/assets"
        unitTest="true"/>
    <sourceProvider
        manifest="src/testRelease/AndroidManifest.xml"
        javaDirectories="src/testRelease/java:src/testRelease/kotlin"
        resDirectories="src/testRelease/res"
        assetsDirectories="src/testRelease/assets"
        unitTest="true"/>
  </testSourceProviders>
  <mainArtifact
      classOutputs="lint/intermediates/javac/release/classes:lint/tmp/kotlin-classes/release:lint/intermediates/compile_r_class_jar/release/R.jar"
      applicationId="com.beeper.android_seshat"
      generatedSourceFolders="lint/generated/ap_generated_sources/release/out:lint/generated/aidl_source_output_dir/release/out:lint/generated/source/buildConfig/release:lint/generated/renderscript_source_output_dir/release/out"
      generatedResourceFolders="lint/generated/res/rs/release:lint/generated/res/resValues/release">
  </mainArtifact>
  <testArtifact>
  </testArtifact>
</variant>
